@model IList<Entities.Males>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Самцы</title>
</head>
<body>
    <h2>Самцы</h2>
    <div>
        @Html.ActionLink("Добавить", "AddMale")
        <hr />
        <dl>
            @foreach (var item in Model)
            {
                <dd>
                        @Html.ActionLink(item.Name, "ShowProfile", new { maleId = item.Id })
                </dd>
            }
        </dl>
    </div>
    <p>
        @Html.ActionLink("На главную", "Index", "Home")
    </p>
</body>
</html>
