@model IList<Entities.Cages>

@{
    ViewBag.Title = "Клетки";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Клетки</h2>

<div>
    @Html.ActionLink("Добавить", "AddCage")
    <hr />
    <dl>
        @foreach (var item in Model)
        {
            <dd>
                @Html.ActionLink(item.Name, "ShowProfile", new {cageId = item.Id})
            </dd>
        }
    </dl>
    <p>
        @Html.ActionLink("На главную", "Index", "Home")
    </p>
</div>

